{
  "title": "晏清元空间",
  "author": "chainheader",
  "description": "",
  "language": "zh-hans",
  "styles": {
    "website": "styles/website.css"
  },
  "collapsible_menu":false,
  "plugins": [
    "katex-ng",
    "theme-comscore",
    "-highlight",
    "prism",
    "copy-code-button",
    "search-pro",
    "-search",
    "-lunr",
    "expandable-chapters",
    "splitter",
    "-sharing",
    "github-buttons",
    "donate",
    "tbfed-pagefooter",
    "anchor-navigation-ex",
    "chapter-fold",
    "mermaid-gb3",
    "simple-mind-map"
  ],
  "pluginsConfig": {
    "github-buttons": {
      "buttons": [
        {
          "user": "m3shine",
          "repo": "m3shine.github.io", 
          "type": "star",
          "count": true,
          "size": "small"
        }
      ]
    },
    "donate": {
      "button": "打赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏",
      "alipay": "https://m3shine.github.io/assets/zfb.jpeg",
      "wechat": "https://m3shine.github.io/assets/wx.png"
    },
    
    
    "prism": {
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ],
      "lang": {
        "shell": "bash"
      }
    },
    "tbfed-pagefooter": {
      "copyright":"转载截图请注明 &copy m3shine.github.io",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "simple-mind-map": {
      "type": "markdown",
      "preset": "colorful",
      "linkShape": "diagonal",
      "autoFit": true
    }
  }
}
